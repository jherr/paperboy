{
  "name": "Paperboy",
  "version": "0.41",
  "description": "Provides integration with NLP analysis engine.",
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["zepto.js", "paperboy.js"],
      "run_at": "document_end"
    }
  ],
  "page_action": {
    "default_icon": {
      "19": "icon-19.png",
      "38": "icon-38.png"
    },
    "default_title": "NLP",
    "default_popup": "popup.html"
  },
  "icons" : {
    "48" : "icon-48.png",
    "128" : "icon-128.png"
  },
  "manifest_version": 2
}
